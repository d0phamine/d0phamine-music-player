{
	"name": "d0phamine-music-player",
	"author": {
		"name": "d0phamine",
		"email": "sizow123@gmail.com"
	},
	"description": "Electron react music player for multiplatform desktop",
	"repository": {
		"type": "git",
		"url": "https://github.com/d0phamine/desktop-react-musicPlayer"
	},
	"license": "GPL-3.0",
	"version": "1.1.0",
	"private": true,
	"build": {
		"appId": "com.d0phamine.d0phamine-music-player",
		"productName": "d0phamine-music-player",
		"copyright": "Copyright Â© 2024",
		"files": [
			"build/**/*",
			"main.js",
			"package.json"
		],
		"directories": {
			"output": "dist"
		},
		"mac": {
			"target": [
				"dmg",
				"zip"
			],
			"category": "public.app-category.productivity",
			"icon": "public/icons/icon.icns"
		},
		"win": {
			"target": [
				{
					"target": "nsis",
					"arch": [
						"x64",
						"arm64"
					]
				}
			],
			"icon": "public/icons/web-app-manifest-512x512.png"
		},
		"linux": {
			"target": [
				{
					"target": "deb",
					"arch": [
						"x64",
						"arm64"
					]
				}
			],
			"category": "Utility",
			"icon": "public/icons/web-app-manifest-512x512.png"
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true
		}
	},
	"devDependencies": {
		"@types/node": "^18.11.10",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@types/react-infinite-scroller": "^1.2.5",
		"@types/react-router-dom": "^5.3.3",
		"@types/three": "^0.169.0",
		"autoprefixer": "^10.4.13",
		"concurrently": "^8.0.1",
		"cross-env": "^7.0.3",
		"electron": "^34.0.2",
		"electron-builder": "^25.1.8",
		"postcss": "^8.4.19",
		"prettier": "^2.8.0",
		"react-scripts": "^5.0.1",
		"rimraf": "^3.0.2",
		"sass": "^1.79.5",
		"sass-loader": "^16.0.2",
		"tailwindcss": "3.2.4",
		"taze": "^0.8.4",
		"typescript": "^4.9.3",
		"wait-on": "^8.0.2"
	},
	"dependencies": {
		"@ant-design/icons": "^5.5.2",
		"@electron/remote": "^2.1.2",
		"@fontsource-variable/jetbrains-mono": "^5.1.1",
		"@fontsource/fira-mono": "^5.1.0",
		"@spotify/web-api-ts-sdk": "^1.2.0",
		"@types/react-howler": "^5.2.3",
		"ajv": "^8.17.1",
		"antd": "^5.21.6",
		"audiomotion-analyzer": "^4.5.0",
		"auto-launch": "^5.0.5",
		"axios": "^1.7.9",
		"cheerio": "^1.0.0",
		"colorthief": "^2.6.0",
		"electron-is-dev": "^2.0.0",
		"electron-reload": "^2.0.0-alpha.1",
		"electron-reloader": "^1.2.3",
		"electron-updater": "^6.3.9",
		"https": "^1.0.0",
		"mobx": "^6.13.3",
		"mobx-react-lite": "^4.0.7",
		"motion": "^11.17.0",
		"music-metadata": "^10.5.1",
		"music-metadata-browser": "^2.5.11",
		"nedb-promises": "^6.2.3",
		"path": "^0.12.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-fast-marquee": "^1.6.5",
		"react-howler": "^5.2.0",
		"react-icons": "^4.12.0",
		"react-infinite-scroller": "^1.2.6",
		"react-router-dom": "^6.4.4",
		"react-toastify": "^9.1.1",
		"react-use": "^17.4.0",
		"use-debounce": "^10.0.4"
	},
	"homepage": "./",
	"main": "public/electron.js",
	"scripts": {
		"dev": "concurrently \"npm run react:dev\" \"npm run electron:wait\"",
		"electron:wait": "wait-on tcp:3000 && npm run electron:dev",
		"prebuild": "rimraf build && rimraf dist",
		"react:build": "react-scripts build",
		"react:dev": "cross-env BROWSER=none react-scripts start",
		"electron:build": "electron-builder build --win --mac --linux --publish never",
		"electron:deploy": "electron-builder build --win --publish always",
		"electron:dev": "electron .",
		"build": "npm run react:build && npm run electron:build",
		"deploy": "npm run react:build && npm run electron:deploy",
		"format": "prettier --write .",
		"update": "taze latest -w"
	},
	"eslintConfig": {
		"extends": [
			"react-app"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
