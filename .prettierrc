{
	"tabWidth": 4,
	"useTabs": true,
	"bracketSpacing": true,
	"singleQuote": false,
	"endOfLine": "crlf",
	"trailingComma": "all",
	"semi": false,
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"^react$",
		"^mobx(.*)$",
		"<THIRD_PARTY_MODULES>",
		"^store",
		"^store/(.*)$",
		"^components/(.*)$",
		"^layout/(.*)$",
		"^/ui/(.*)$",
		"^/logic/(.*)$",
		"^pages/(.*)$",
		"^logic/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
